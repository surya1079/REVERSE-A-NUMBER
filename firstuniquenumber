def firstuniqchar(self,s:str):
    d={}
    for i in range(len(s)):
        if i in d:
            d[s[i]]+=1
        else:
            d[s[i]]=1
    for key,values in d.items():
        if value==1:
            return s.index(key)
    return-1
s=input("Enter the string")
print(firstuniqchar(s))                    
